{
	"name": "@philippdormann/mensamax-api",
	"version": "2.3.0",
	"license": "MIT",
	"description": "MensaMax-API. Simple APIs for using MensaMax data in modern applications",
	"main": "main.js",
	"scripts": {
		"release": "release-it",
		"test": "ava",
		"start": "node server.js",
		"dev": "nodemon server.js",
		"format": "prettier --write ."
	},
	"dependencies": {
		"axios": "0.27.2",
		"cheerio": "1.0.0-rc.12",
		"dotenv": "16.0.2",
		"html-minifier": "4.0.0",
		"ioredis": "^5.2.3",
		"path": "0.12.7",
		"rayo": "^1.3.10",
		"request": "2.88.2"
	},
	"devDependencies": {
		"auto-changelog": "2.4.0",
		"ava": "4.3.3",
		"nodemon": "2.0.20",
		"prettier": "2.7.1",
		"release-it": "15.4.2"
	},
	"release-it": {
		"git": {
			"commit": true,
			"requireBranch": "main",
			"requireCleanWorkingDir": false,
			"commitMessage": "ðŸš€RELEASE v${version}",
			"push": true,
			"tag": true,
			"tagName": "v${version}",
			"tagAnnotation": "v${version}"
		},
		"npm": {
			"publish": false
		},
		"hooks": {
			"after:bump": "auto-changelog --commit-limit false -p -u --hide-credit && prettier --write . && git add ."
		}
	}
}
